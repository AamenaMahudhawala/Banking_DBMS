Database Name: aamena_db:
Create Database aamena_db;
Use aamena_db;
Table Name: Accounts
mysql -u <username> -h <hostname>

Code:
CREATE TABLE Accounts(CODE CHAR(50) PRIMARY KEY,FIRSTNAME VARCHAR(50),LASTNAME VARCHAR(50),PHONENO VARCHAR(10),IDENTIFICATION VARCHAR(50),CURRENTAMT FLOAT(3),TYPE VARCHAR(5),TENURE FLOAT(1),INTEREST_RATE FLOAT(2),ACCOUNT_CREATED_ON_DATE,MATURITY_DATE DATE,STATUS VARCHAR(25));

Table Name:	AccountsandPassword
Code:
Code:
CREATE TABLE AccountsandPassword(Code CHAR(50) PRIMARY KEY,PASSWORD VARCHAR(50),STATUS VARCHAR(25),FOREIGN KEY (Code) REFERENCES Accounts(CODE));
